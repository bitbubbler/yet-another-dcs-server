{
  "devDependencies": {
    "@grpc/grpc-js": "^1.9.7",
    "@grpc/proto-loader": "^0.6.13",
    "@mikro-orm/cli": "^5.8.10",
    "@types/adm-zip": "^0.5.3",
    "@types/geodesy": "^2.2.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "ts-node --compilerOptions {\\\"module\\\":\\\"CommonJS\\\"} src/bin/server.ts",
    "dump-template": "ts-node --compilerOptions {\\\"module\\\":\\\"CommonJS\\\"} src/bin/dump-template.ts",
    "test": "jest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prettier": "npx prettier --check src/",
    "full-build": "npm run codegen && npm run build",
    "build": "rm -rf build/ && tsc && cp -r src/proto/ build/ && cp -r package*.json build/",
    "codegen": "node generate-types.js && mikro-orm cache:generate"
  },
  "dependencies": {
    "@mikro-orm/core": "^5.8.10",
    "@mikro-orm/migrations": "^5.8.10",
    "@mikro-orm/reflection": "^5.8.10",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@mikro-orm/sqlite": "^5.8.10",
    "adm-zip": "^0.5.10",
    "commander": "^9.5.0",
    "exponential-backoff": "^3.1.1",
    "fuzzysort": "^2.0.4",
    "geodesy": "^2.4.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^8.3.2"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./build/mikro-orm.config.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0 <=21.0.0"
  }
}
