{
  "devDependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.12",
    "@mikro-orm/cli": "^5.6.3",
    "@types/adm-zip": "^0.5.0",
    "@types/geodesy": "^2.2.3",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "start": "ts-node --compilerOptions {\\\"module\\\":\\\"CommonJS\\\"} src/bin/server.ts",
    "say": "ts-node --compilerOptions {\\\"module\\\":\\\"CommonJS\\\"} src/bin/test.ts",
    "dump-template": "ts-node --compilerOptions {\\\"module\\\":\\\"CommonJS\\\"} src/bin/dump-template.ts",
    "test": "jest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prettier": "npx prettier --check src/",
    "full-build": "npm run codegen && npm run build",
    "build": "rm -rf build/ && tsc && cp -r src/proto/ build/ && cp -r package*.json build/",
    "codegen": "node generate-types.js && mikro-orm cache:generate"
  },
  "dependencies": {
    "@mikro-orm/better-sqlite": "^5.6.3",
    "@mikro-orm/core": "^5.6.3",
    "@mikro-orm/migrations": "^5.6.3",
    "@mikro-orm/reflection": "^5.6.4",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "adm-zip": "^0.5.9",
    "better-sqlite3": "^7.5.3",
    "commander": "^9.3.0",
    "exponential-backoff": "^3.1.0",
    "fuzzysort": "^2.0.1",
    "geodesy": "^2.1.0",
    "knex": "^2.1.0",
    "pino": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "uuid": "^8.3.2"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./build/mikro-orm.config.js"
    ]
  }
}
